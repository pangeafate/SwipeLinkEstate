{
  "name": "swipe-link-estate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "playwright test",
    
    "db:migrate:crm": "node scripts/apply-crm-schema-simple.mjs apply",
    "db:migrate:crm:check": "node scripts/apply-crm-schema-simple.mjs check",
    "db:migrate:crm:status": "node scripts/apply-crm-schema-simple.mjs check",
    "db:migrate:crm:verify": "node scripts/apply-crm-schema-simple.mjs check",
    "db:test:crm": "node scripts/test-crm-schema.mjs all",
    "db:test:crm:migrate": "node scripts/test-crm-schema.mjs migrate",
    "db:test:crm:seed": "node scripts/test-crm-schema.mjs seed",
    "db:test:crm:perf": "node scripts/test-crm-schema.mjs perf",
    
    "db:setup:dev": "npm run db:migrate:crm && npm run db:seed:crm && npm run db:test:crm",
    "db:reset:dev": "npm run db:migrate:crm:rollback && npm run db:migrate:crm && npm run db:seed:crm"
  },
  "dependencies": {
    "@react-spring/web": "^9.7.5",
    "@supabase/supabase-js": "^2.39.0",
    "@tanstack/react-query": "^5.0.0",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.0",
    "lucide-react": "^0.540.0",
    "next": "^14.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hot-toast": "^2.4.0",
    "react-tinder-card": "^1.6.2",
    "tailwind-merge": "^2.0.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@tanstack/react-query-devtools": "^5.85.5",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0"
  }
}
